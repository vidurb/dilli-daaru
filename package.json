{
  "name": "dilli-daaru",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:pull": "dotenv -e .env.local prisma db pull",
    "db:push": "dotenv -e .env.local prisma db push",
    "db:seed": "dotenv -e .env.local prisma db seed",
    "db:studio": "dotenv -e .env.local prisma studio",
    "db:generate": "dotenv -e .env.local prisma generate",
    "db:reset:dev": "dotenv -e .env.local prisma migrate reset",
    "db:migrate:dev": "dotenv -e .env.local prisma migrate dev",
    "db:pull:prod": "dotenv -e .env.prod.local prisma db pull",
    "db:push:prod": "dotenv -e .env.prod.local prisma db push",
    "db:seed:prod": "dotenv -e .env.prod.local prisma db seed",
    "db:studio:prod": "dotenv -e .env.prod.local prisma studio",
    "db:reset:prod": "dotenv -e .env.prod.local prisma migrate reset",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "@phosphor-icons/react": "^2.0.15",
    "@prisma/client": "5.8.1",
    "@sentry/nextjs": "^7.98.0",
    "@types/node": "20.11.7",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@vercel/analytics": "^1.1.2",
    "autoprefixer": "10.4.17",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "next": "14.1.0",
    "postcss": "8.4.33",
    "radash": "^11.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-loading-skeleton": "^3.3.1",
    "short-uuid": "^4.2.2",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@faker-js/faker": "^8.4.0",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.6.10",
    "@storybook/nextjs": "^7.6.10",
    "@storybook/react": "^7.6.10",
    "@storybook/testing-library": "^0.2.2",
    "@types/geojson": "^7946.0.13",
    "chromatic": "^10.6.0",
    "decimal.js": "^10.4.3",
    "dotenv-cli": "^7.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "husky": "^9.0.6",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "prisma-docs-generator": "^0.8.0",
    "prisma-generator-fake-data": "^0.14.1",
    "prisma-json-schema-generator": "^5.1.1",
    "sass": "^1.70.0",
    "storybook": "^7.6.10",
    "stylelint": "^16.2.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "ts-node": "^10.9.2"
  },
  "prisma": {
    "seed": "sh prisma/seed.sh"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard-scss",
      "stylelint-config-tailwindcss/scss"
    ],
    "rules": {
      "selector-class-pattern": null
    },
    "ignoreFiles": [
      "prisma/docs/**"
    ]
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier",
      "plugin:storybook/recommended"
    ],
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "simple-import-sort/imports": "error",
      "simple-import-sort/exports": "error"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  }
}
